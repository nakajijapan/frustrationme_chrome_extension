{
  "name": "www.frustration.me extension",
  "description" : "ソーシャルウィッシュリスト「www.frustration.me」のchrome extensionです。Amazon、楽天、ヤフーオークション、iTunesの5つのAPIを利用してアイテムの検索を行うことができます。",
  "version": "2.0.6",
  "manifest_version": 2,
  "icons": {
    "16": "24.png",
    "24": "24.png",
    "128": "128.png"
  },
  "browser_action":{
    "default_icon": "24.png",
    "default_title": "www.frustartion.me",
    "default_popup":"popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources" : [
    "js/jquery.js",
    "js/content_script.js",
    "js/common.js",
    "js/background.js",
    "js/popup.js"
  ],
  "permissions" : [
    "tabs",
    "contextMenus",
    "background",
    "http://*/*"
  ]
}
